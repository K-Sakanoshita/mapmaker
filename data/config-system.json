{
    "icon": {
        "path": "maki-icon"
    },
    "system": {
        "OverPassServer": [
            "https://maps.mail.ru/osm/tools/overpass/api/interpreter",
            "https://overpass.openstreetmap.jp/api/interpreter",
            "https://overpass.kumi.systems/api/interpreter",
            "https://overpass-api.de/api/interpreter"
        ]
    },
    "tile_select": {
        "Facebook_Builtin": "OSM_Mono",
        "default": "OSM_Mono"
    },
    "tile": {
        "OSM_Mono": {
            "name": "OpenStreetMap Mono",
            "copyright": "<a href=\"https://openstreetmap.org\">&copy OpenStreetMap contributors</a>",
            "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
            "filter": [
                "grayscale:90%",
                "bright:80%",
                "contrast: 150%",
                "sepia: 0%"
            ],
            "maxNativeZoom": 18
        },
        "OSM_Standard": {
            "name": "OpenStreetMap Standard",
            "copyright": "<a href=\"https://openstreetmap.org\">&copy OpenStreetMap contributors</a>",
            "url": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png",
            "maxNativeZoom": 18
        },
        "GSI_pale": {
            "name": "地理院地図 淡色",
            "copyright": "<a href='https://www.gsi.go.jp/kikakuchousei/kikakuchousei40182.html'>国土地理院 | &copy OpenStreetMap contributors</a>",
            "url": "https://cyberjapandata.gsi.go.jp/xyz/pale/{z}/{x}/{y}.png",
            "maxNativeZoom": 18
        },
        "GSI_ort": {
            "name": "地理院地図 オルソ",
            "copyright": "<a href='https://www.gsi.go.jp/kikakuchousei/kikakuchousei40182.html'>国土地理院 | &copy OpenStreetMap contributors</a>",
            "url": "https://cyberjapandata.gsi.go.jp/xyz/ort/{z}/{x}/{y}.jpg",
            "maxNativeZoom": 18
        }
    },
    "menu_list": {
        "template": "<a class='dropdown-item'><span></span></a><span class='dropdown-item'></span>",
        "divider": "<div class='dropdown-divider'></div>"
    },
    "effect": {
        "icon": {
            "stack": "./image/icon_circle.svg",
            "x": 0,
            "y": 0
        },
        "text": {
            "size": 12,
            "view": true
        }
    },
    "svg": {
        "circle": {
            "radius": 8,
            "color": "#000080",
            "fillColor": "#0000A0",
            "Opacity": 0.1,
            "fillOpacity": 0.1,
            "timer": 10000000
        },
        "text": {
            "view": true,
            "size": 14,
            "color": "black"
        }
    }
}